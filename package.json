{
  "name": "betterads",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "dev:full": "npm run db:start && npm run prisma:generate && npm run dev",
    "db:start": "docker-compose up -d postgres pgadmin",
    "db:stop": "docker-compose stop postgres pgadmin",
    "db:restart": "docker-compose restart postgres pgadmin",
    "db:logs": "docker-compose logs -f postgres",
    "db:clean": "docker-compose down -v --remove-orphans && docker system prune -f",
    "docker:build": "docker-compose build app",
    "docker:prod": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "prisma:generate": "prisma generate",
    "prisma:push": "prisma db push",
    "prisma:studio": "prisma studio",
    "prisma:reset": "prisma migrate reset --force",
    "setup:local": "npm run db:start && sleep 10 && npm run prisma:generate && npm run prisma:push"
  },
  "dependencies": {
    "@better-auth/cli": "^1.3.4",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@prisma/client": "^6.13.0",
    "@tabler/icons-react": "^3.34.1",
    "better-auth": "^1.3.4",
    "better-auth/adapters/prisma": "^1.3.4",
    "clsx": "^2.1.1",
    "geist": "^1.4.2",
    "lucide-react": "^0.536.0",
    "next": "15.4.5",
    "next-themes": "^0.4.6",
    "prisma": "^6.13.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.4.5",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.3.6",
    "typescript": "^5"
  }
}
